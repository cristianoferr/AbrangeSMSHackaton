{
    "_version": "{{appVersion}}",
    "start_url": "index.html",
    "sap.app": {
        "id": "templateHackaton",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "{{appVersion}}"
        },
        "title": "{{appTitulo}}",
        "description": "{{appDescricao}}",
        "sourceTemplate": {
            "id": "ui5template.basicSAPUI5ApplicationProject",
            "version": "1.38.11"
        },
        "dataSources": {
            "deviceDs": {
                "type": "JSON"
            },
            "viewModelDs": {
                "type": "JSON"
            },
            "mainService": {
                "uri": "/api",
                "type": "JSON"
            },
            "init_dominio": {
                "uri": "model/dominio.json",
                "type": "JSON"
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_bluecrystal",
            "sap_belize"
        ]
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "templateHackaton.layout.app",
            "type": "XML"
        },
        "dependencies": {
            "minUI5Version": "1.30.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {},
                "sap.uxap": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "templateHackaton.i18n.i18n"
                }
            },
            "dominio": {
                "type": "sap.ui.model.json.JSONModel",
                "dataSource": "init_dominio"
            },
            "": {
                "type": "templateHackaton.shared.model.RESTModel",
                "dataSource": "mainService",
                "settings": {
                    "listProperty": "",
                    "listSuffix": ""
                }
            },
            "device": {
                "type": "sap.ui.model.json.JSONModel",
                "dataSource": "deviceDs",
                "settings": {
                    "defaultBindingMode": "OneWay"
                }
            },
            "viewModel": {
                "type": "sap.ui.model.json.JSONModel",
                "dataSource": "viewModelDs",
                "settings": {
                    "defaultBindingMode": "TwoWay"
                }
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "controlId": "app",
                "controlAggregation": "pages",
                "transition": "slide",
                "bypassed": {
                    "target": "notFound"
                }
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "routeAppHome",
                    "target": "tgHome"
                },
                {
                    "pattern": "novoAlerta",
                    "name": "routeNovoAlerta",
                    "parent": "routeAppHome",
                    "target": "tgNovoAlerta"
                },
                {
                    "pattern": "estados",
                    "name": "routeConsultaEstados",
                    "target": [
                        "tgHome",
                        "tgConsultaEstados"
                    ]
                },
                {
                    "pattern": "estado/{id}",
                    "name": "routeConsultaEstado",
                    "parent": "routeConsultaEstados",
                    "target": "tgConsultaEstado"
                }
            ],
            "targets": {
                "tgHome": {
                    "viewPath": "templateHackaton.domain.home",
                    "viewName": "home",
                    "viewLevel": 1
                },
                "tgNovoAlerta": {
                    "viewPath": "templateHackaton.domain.alerta.registro",
                    "viewName": "registro",
                    "viewLevel": 2
                },
                "tgConsultaEstados": {
                    "viewPath": "templateHackaton.domain.estados.consulta",
                    "viewName": "estados",
                    "parent": "masterDetailSimplesSplitApp",
                    "controlId": "masterDetailSimplesSplitAppId",
                    "controlAggregation": "masterPages",
                    "viewLevel": 1
                },
                "tgConsultaEstado": {
                    "viewPath": "templateHackaton.domain.estados.consulta",
                    "viewName": "estado",
                    "parent": "masterDetailSimplesSplitApp",
                    "controlId": "masterDetailSimplesSplitAppId",
                    "controlAggregation": "detailPages",
                    "viewLevel": 2
                },
                "masterDetailSimplesSplitApp": {
                    "viewPath": "templateHackaton.layout",
                    "viewName": "masterDetailSimplesSplit"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "content/css/style.css"
                },
                {
                    "uri": "domain/alerta/registro/1_tipoAlerta/tipoAlerta.css"
                },
                {
                    "uri": "domain/alerta/registro/3_regrasOuro/regrasOuro.css"
                }
            ],
            "js": [
                {
                    "uri": "domain/estados/estadoService.js",
                    "id": "estadoService"
                },
                {
                    "uri": "domain/alerta/registro/1_tipoAlerta/tipoAlertaService.js",
                    "id": "tipoAlertaService"
                },
                {
                    "uri": "domain/alerta/registro/2_dadosAlerta/dadosAlertaService.js",
                    "id": "dadosAlertaService"
                },
                
                {
                    "uri": "shared/moment.min.js",
                    "id": "momentjs"
                },
                {
                    "uri": "shared/dialogService.js",
                    "id": "dialogService"
                },
                {
                    "uri": "shared/formatter.js",
                    "id": "formatter"
                },
                {
                    "uri": "domain/services/pendenciaService.js",
                    "id": "pendenciaService"
                },
                {
                    "uri": "domain/services/backendService.js",
                    "id": "backendService"
                },
                {
                    "uri": "shared/toolsService.js",
                    "id": "toolsService"
                },
                {
                    "uri": "shared/constants.js",
                    "id": "constants"
                },
                {
                    "uri": "shared/initializerService.js",
                    "id": "initializerService"
                },
                {
                    "uri": "shared/seguranca/userService.js",
                    "id": "userService"
                }
            ]
        }
    }
}